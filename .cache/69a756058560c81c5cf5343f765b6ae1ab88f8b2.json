        totalKHR += o.khr\n        totalUSD += o.usd\n      })\n\n      this.totalQty = totalQty\n      this.totalUsd = totalUSD\n      this.form.totalUsd = totalUSD\n      this.totalKhr = totalKHR\n      this.form.totalKhr = totalKHR\n\n      this.items = items\n    },\n    saveForm(childValid) {\n      this.$refs['form'].validate(valid => {\n        if (valid && childValid) {\n          this.form.tranDate = wrapCurrentTime(this.form.tranDate)\n          insertIncome\n            .callPromise({ doc: this.form, details: this.items })\n            .then(result => {\n              if (result) {\n                Msg.success()\n                this.cancelForm()\n              }\n            })\n            .catch(error => {\n              Notify.error({ message: error })\n            })\n        } else {\n          return false\n        }\n      })\n    },\n    cancelForm() {\n      this.$refs['form'].resetFields()\n      this.itemsProp = this.initItems()\n    },\n    handleClose() {\n      this.$emit('modal-close')\n    },\n    handleresetForm() {\n      this.$refs['form'].resetFields()\n    },\n    formatNum(val) {\n      return numeral(val).format('0,0.00')\n    },\n  },\n}\n"]},"sourceType":"script","hash":"69a756058560c81c5cf5343f765b6ae1ab88f8b2"}
